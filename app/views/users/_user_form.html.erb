<%= form_for @user do |f| %>
    <% if @user.errors.any? %>
<ul>
   <% @user.errors.full_messages.each do |m| %>
  <li><%= m %></li>
    <% end %>
</ul>
  <% end %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :gear %>
  <%= f.text_field :gear %>
  <%= f.label :photo %>
  <%= f.text_field :photo %>
  <%= f.label :caption %>
  <%= f.text_area :caption %>
  <%= f.submit %>
<% end %>

